{
  "name": "react-dxf-viewer",
  "version": "0.1.0",
  "private": true,
  "description": "React component for viewing DXF files with area selection and coordinate extraction",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "keywords": [
    "react",
    "dxf",
    "cad",
    "viewer",
    "area-selection",
    "coordinates",
    "canvas",
    "typescript",
    "hooks",
    "interactive"
  ],
  "author": "CreamCheese-Ja",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CreamCheese-Ja/react-dxf-viewer.git"
  },
  "homepage": "https://github.com/CreamCheese-Ja/react-dxf-viewer#readme",
  "bugs": {
    "url": "https://github.com/CreamCheese-Ja/react-dxf-viewer/issues"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.5.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --apply src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "type-check": "tsc --noEmit",
    "example": "cd example && npm run dev",
    "example:install": "cd example && npm install",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.46.2",
    "rollup-plugin-postcss": "^4.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3"
  }
}
